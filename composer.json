{
	"name": "evoltix/home-sitemap",
	"description": "Home page internal links sitemap",
	"homepage": "https://github.com/evoltix/home-sitemap",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Evoltix",
			"email": "avneng.negi@gmail.com",
			"homepage": "https://github.com/evoltix"
		}
	],
	"type": "Plugin",
	"config": {
		"sort-packages": true
	},
	"support": {
		"issues": "https://github.com/evoltix/home-sitemap/issues",
		"source": "https://github.com/evoltix/home-sitemap"
	},
	"require-dev": {
		"php": "^5.6 || ^7",
		"brain/monkey": "^2.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"phpcompatibility/phpcompatibility-wp": "^2.0",
		"phpunit/phpunit": "^5.7 || ^7",
		"wp-coding-standards/wpcs": "^2",
		},
	"autoload": {},
	"autoload-dev": {},
	"scripts": {
		"test-unit": "\"vendor/bin/wpmedia-phpunit\" unit path=Tests/Unit",
		"test-integration": "\"vendor/bin/wpmedia-phpunit\" integration path=Tests/Integration/",
		"run-tests": [
			"@test-unit",
			"@test-integration"
		],
		"install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
		"phpcs": "phpcs --basepath=.",
		"phpcs-changed": "./bin/phpcs-changed.sh",
		"phpcs:fix": "phpcbf"
	}
}
